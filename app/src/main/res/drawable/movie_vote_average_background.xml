<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="ring" android:innerRadius="@dimen/movie_vote_average_outer_radius"
            android:thickness="@dimen/movie_vote_average_outer_tickness"
            android:useLevel="false">
            <solid android:color="@color/primary_dark_color" />
            <size android:height="@dimen/movie_vote_average_circle_size"
                android:width="@dimen/movie_vote_average_circle_size"/>
        </shape>
    </item>
    <item android:top="@dimen/movie_vote_average_inner_margin"
        android:left="@dimen/movie_vote_average_inner_margin"
        android:right="@dimen/movie_vote_average_inner_margin"
        android:bottom="@dimen/movie_vote_average_inner_margin">
        <shape android:shape="oval">
            <solid android:color="@color/primary_dark_color" />
            <size android:height="@dimen/movie_vote_average_circle_size"
                android:width="@dimen/movie_vote_average_circle_size"/>
        </shape>
    </item>
</layer-list>